        "class Human:\n",
        "  def __init__(self):\n",
        "    self.name = \"\"\n",
        "    self.age = 0\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(\"Name: \", self.name)\n",
        "    print(\"Age: \", self.age)\n",
        "\n",
        "human = Human()\n",
        "human.name = \"太郎\"\n",
        "human.age = 8\n",
        "human.printinfo()"
